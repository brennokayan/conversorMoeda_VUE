/projeto teste pra ver se tá instalado o vue/teste1/src/components/Converso.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"conversor\">\n        <h2>{{moedaA}} Para {{moedaB}}</h2>\n        <input type=\"text\" v-model=\"moedaA_value\" v-bind:placeholder=\"moedaA\">\n        <input type=\"button\" value=\"Converter\" v-on:click=\"converterus\">\n        <h2>{{moedaB_value}}</h2>\n        <h2>{{url}}</h2>\n    </div>\n    <div class=\"conversor1\">\n        <h2>BRL Para USD</h2>\n        <input type=\"text\" v-model=\"moedaAbr_value\" v-bind:placeholder=\"moedaA\">\n        <input type=\"button\" value=\"Converter\" v-on:click=\"converterbr\">\n        <h2>{{moedaB_value}}</h2>\n        <h2>{{url}}</h2>\n    </div>\n</template>\n\n<script>\nexport default {\n    name:'conversor',\n    props: ['moedaA','moedaB'],\n            data(){\n                return{\n                    moedaA_value : \"\",\n                    moedaB_value : 0,\n                    moedaAbr_value : \"\",\n                    moedaBbr_value : 0,\n                };\n            },\n    methods : {\n        converterus(){\n            let de_para = this.moedaA +\"-\"+this.moedaB;\n            let url = \"https://economia.awesomeapi.com.br/all/\"+de_para;\n\n            fetch(url).then(res=>{return res.json()})\n                      .then(json=>{\n                          let cotacao = cotacao = json.USD.high;  \n                          this.moedaB_value =( cotacao*parseFloat(this.moedaA_value)).toFixed(2);\n                      })\n        },\n        converterbr(){\n            \n            let url = \"https://economia.awesomeapi.com.br/all/USD-BRL\";\n\n            fetch(url).then(res=>{return res.json()})\n                      .then(json=>{\n                          let cotacao = cotacao = json.USD.high;  \n                          this.moedaBbr_value =( cotacao/parseFloat(this.moedaAbr_value)).toFixed(2);\n                      })\n        },\n        \n\n    }\n}\n</script>\n\n<style scoped>\n.conversor{\n    max-width: 300px;\n    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);\n    padding: 20px;\n}\n.conversor1{\n    max-width: 300px;\n    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);\n    padding: 20px;\n}\n\n</style>"]}]}